package com.michael.microservices.driversservice.bean;

public class Address {
	String country;
	String city;
	String street;
	Integer houseNum;
	Integer apartmentNum;
	
	
	public Address(String country, String city, String street, Integer houseNum, Integer apartmentNum) {
		super();
		this.country = country;
		this.city = city;
		this.street = street;
		this.houseNum = houseNum;
		this.apartmentNum = apartmentNum;
	}
	
	public String getCountry() {
		return country;
	}
	public void setCountry(String country) {
		this.country = country;
	}
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	public String getStreet() {
		return street;
	}
	public void setStreet(String street) {
		this.street = street;
	}
	public Integer getHouseNum() {
		return houseNum;
	}
	public void setHouseNum(Integer houseNum) {
		this.houseNum = houseNum;
	}
	public Integer getApartmentNum() {
		return apartmentNum;
	}
	public void setApartmentNum(Integer apartmentNum) {
		this.apartmentNum = apartmentNum;
	}
}
